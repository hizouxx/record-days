/* miniprogram/pages/setting/setting.wxss */

.bg-item{
  width: 150rpx;
  height: 300rpx;
  position: relative;
}

.bg-item image{
  position: absolute;
  top: 50%;
  left: 50%;
  height: 100%;
  width: 100%;
  transform: translate(-50%, -50%);
}

.bg-item-check{
  position: absolute;
  right: 5rpx;
  top: 5rpx;
  z-index: 9;
}